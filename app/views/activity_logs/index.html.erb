<p id="notice"><%= notice %></p>

<h1>Listing Activity Logs</h1>

<table>
  <thead>
    <tr>
      <th>Currentactivity</th>
      <th>Currentmood</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_logs.each do |activity_log| %>
      <tr>
        <td><%= activity_log.CurrentActivity %></td>
        <td><%= activity_log.CurrentMood %></td>
        <td><%= link_to 'Show', activity_log %></td>
        <td><%= link_to 'Edit', edit_activity_log_path(activity_log) %></td>
        <td><%= link_to 'Destroy', activity_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity log', new_activity_log_path %>
